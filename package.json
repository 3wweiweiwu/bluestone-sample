{
    "name": "bluestone-sample-project",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "mocha ./script/**.js --timeout 60000s --exit --grep '^.*smoke.*' --reporter mocha/lib/reporters/spec.js --reporter-option 'output=testrun.json,BLUESTONE_RUN_ID=testrun123,BLUESTONE_AUTO_SNAPSHOT=1' --retries 2",
        "build": "babel bin -d dist",
        "test-sample": "node ./node_modules/mocha/bin/_mocha ./script/*.js --exit true --grep \"^test1 test2$\" --timeout 60000s"
    },
    "dependencies": {
        "bluestone": "^0.5.33",
        "mocha": "^10.1.0"
    }
}